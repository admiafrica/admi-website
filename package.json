    "@rematch/core": "^2.2.0",
    "@rematch/loading": "^2.1.2",
    "@tabler/icons-react": "^3.16.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.11.0",
    "chroma-js": "^3.1.1",
    "clsx": "^2.1.1",
    "contentful": "^11.2.5",
    "contentful-management": "^11.54.3",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.0",
    "embla-carousel-autoplay": "^7.1.0",
